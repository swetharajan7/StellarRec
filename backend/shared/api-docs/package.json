{
  "name": "@stellarrec/api-docs",
  "version": "1.0.0",
  "description": "Comprehensive API documentation for StellarRec platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "node dist/scripts/generate-docs.js",
    "serve": "node dist/server/documentation-server.js",
    "validate": "swagger-codegen validate -i ./specs/openapi.yaml",
    "test": "jest"
  },
  "dependencies": {
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.0",
    "openapi-generator-cli": "^2.7.0",
    "redoc-cli": "^0.13.0",
    "express": "^4.18.0",
    "cors": "^2.8.0",
    "helmet": "^7.1.0",
    "yaml": "^2.3.0",
    "mustache": "^4.2.0",
    "markdown-it": "^14.0.0",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.0",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/cors": "^2.8.0",
    "@types/mustache": "^4.2.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "api-documentation",
    "openapi",
    "swagger",
    "rest-api",
    "developer-tools",
    "code-generation"
  ],
  "author": "StellarRec Team",
  "license": "MIT"
}