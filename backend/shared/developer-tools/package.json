{
  "name": "@stellarrec/developer-tools",
  "version": "1.0.0",
  "description": "Comprehensive developer tools and SDKs for StellarRec platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stellarrec": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "generate-sdks": "node dist/scripts/generate-all-sdks.js",
    "start-portal": "node dist/portal/server.js",
    "start-mock": "node dist/testing/mock-server.js"
