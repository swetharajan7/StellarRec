    "yaml": "^2.3.0",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.18.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/mustache": "^4.2.0",
    "typescript": "^4.9.0",
    "jest": "^27.5.0",
    "@types/jest": "^27.5.0",
    "ts-jest": "^27.1.0"
  },
  "keywords": [
    "sdk",
    "api-client",
