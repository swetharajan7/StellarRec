{\n  \"name\": \"stellarrec-frontend\",\n  \"version\": \"1.0.0\",\n  \"description\": \"StellarRec - AI-Powered College Application Platform Frontend\",\n  \"main\": \"index.html\",\n  \"scripts\": {\n    \"start\": \"npx http-server . -p 3000 -c-1\",\n    \"build\": \"npm run build:prod\",\n    \"build:prod\": \"npm run minify && npm run optimize\",\n    \"build:staging\": \"npm run minify\",\n    \"build:dev\": \"echo 'Development build - no processing needed'\",\n    \"minify\": \"npx html-minifier-terser --input-dir . --output-dir dist --file-ext html --collapse-whitespace --remove-comments --minify-css --minify-js\",\n    \"optimize\": \"npm run optimize:images && npm run optimize:js\",\n    \"optimize:images\": \"npx imagemin '**/*.{jpg,jpeg,png,gif,svg}' --out-dir=dist\",\n    \"optimize:js\": \"npx terser frontend/js/*.js --compress --mangle --output dist/frontend/js/app.min.js\",\n    \"test\": \"npx jest\",\n    \"lint\": \"npx eslint frontend/js/*.js\",\n    \"format\": \"npx prettier --write '**/*.{js,html,css,json}'\",\n    \"deploy\": \"netlify deploy --prod\",\n    \"deploy:preview\": \"netlify deploy\",\n    \"analyze\": \"npx webpack-bundle-analyzer dist/static/js/*.js\",\n    \"lighthouse\": \"npx lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html\",\n    \"validate\": \"npx html-validate *.html\"\n  },\n  \"keywords\": [\n    \"college-applications\",\n    \"ai-powered\",\n    \"university-matching\",\n    \"recommendation-letters\",\n    \"education-technology\",\n    \"student-platform\"\n  ],\n  \"author\": \"StellarRec Team\",\n  \"license\": \"MIT\",\n  \"repository\": {\n    \"type\": \"git\",\n    \"url\": \"https://github.com/swetharajan7/StellarRec.git\"\n  },\n  \"homepage\": \"https://stellarrec.com\",\n  \"devDependencies\": {\n    \"@netlify/cli\": \"^17.0.0\",\n    \"eslint\": \"^8.50.0\",\n    \"html-minifier-terser\": \"^7.2.0\",\n    \"html-validate\": \"^8.5.0\",\n    \"http-server\": \"^14.1.1\",\n    \"imagemin\": \"^8.0.1\",\n    \"imagemin-cli\": \"^7.0.0\",\n    \"jest\": \"^29.7.0\",\n    \"lighthouse\": \"^11.2.0\",\n    \"prettier\": \"^3.0.3\",\n    \"terser\": \"^5.22.0\",\n    \"webpack-bundle-analyzer\": \"^4.9.1\"\n  },\n  \"dependencies\": {\n    \"chart.js\": \"^4.4.0\",\n    \"date-fns\": \"^2.30.0\",\n    \"lodash\": \"^4.17.21\"\n  },\n  \"browserslist\": [\n    \"> 1%\",\n    \"last 2 versions\",\n    \"not dead\",\n    \"not ie 11\"\n  ],\n  \"engines\": {\n    \"node\": \">=16.0.0\",\n    \"npm\": \">=8.0.0\"\n  },\n  \"jest\": {\n    \"testEnvironment\": \"jsdom\",\n    \"setupFilesAfterEnv\": [\"<rootDir>/frontend/js/test-setup.js\"],\n    \"testMatch\": [\"<rootDir>/frontend/js/**/*.test.js\"],\n    \"collectCoverageFrom\": [\n      \"frontend/js/**/*.js\",\n      \"!frontend/js/**/*.test.js\",\n      \"!frontend/js/test-setup.js\"\n    ],\n    \"coverageThreshold\": {\n      \"global\": {\n        \"branches\": 80,\n        \"functions\": 80,\n        \"lines\": 80,\n        \"statements\": 80\n      }\n    }\n  },\n  \"eslintConfig\": {\n    \"env\": {\n      \"browser\": true,\n      \"es2021\": true\n    },\n    \"extends\": \"eslint:recommended\",\n    \"parserOptions\": {\n      \"ecmaVersion\": \"latest\",\n      \"sourceType\": \"module\"\n    },\n    \"rules\": {\n      \"no-unused-vars\": \"warn\",\n      \"no-console\": \"warn\",\n      \"prefer-const\": \"error\",\n      \"no-var\": \"error\"\n    },\n    \"globals\": {\n      \"StellarRecAPI\": \"readonly\",\n      \"DashboardController\": \"readonly\",\n      \"StellaChatbot\": \"readonly\"\n    }\n  },\n  \"prettier\": {\n    \"semi\": true,\n    \"trailingComma\": \"es5\",\n    \"singleQuote\": true,\n    \"printWidth\": 100,\n    \"tabWidth\": 2\n  }\n}"